{
  "expo": {
    "name": "React Native FlatList Sticky Item",
    "description": "An example of a sticky item (row) for the React Native FlatList",
    "githubUrl": "https://github.com/vladimir-vovk/react-native-flatlist-sticky-item",
    "slug": "react-native-flatlist-sticky-item",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
